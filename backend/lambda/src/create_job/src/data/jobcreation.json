{
  "companyId": {
    "type": "string",
    "required": true
  },
  "contact": {
    "name": {
      "type": "string",
      "required": true
    },
    "phoneNumber": {
      "type": "string",
      "required": true
    }
  },
  "location": {
    "street": {
      "type": "string",
      "required": true
    },
    "city": {
      "type": "string",
      "required": false
    },
    "zipCode": {
      "type": "string",
      "match": "/[0-9]+/",
      "required": false
    },
    "state": {
      "type": "string",
      "required": true
    }
  },
  "targetSchedule": {
    "scheduleType": {
      "type": "string",
      "required": false
    },
    "startDate": {
      "type": "Date",
      "required": false
    },
    "endDate": {
      "type": "Date",
      "required": false
    },
    "time": {
      "start": {
        "type": "Date",
        "required": false
      },
      "end": {
        "type": "Date",
        "required": false
      }
    }
  }
}
